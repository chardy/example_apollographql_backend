{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.4.0"
        }
      }
    ]
  ],
  "plugins": [
    ["transform-object-rest-spread"],
    ["module-resolver",
      {
        "root": ["."],
        "alias": {
          "root": "./",
          "src": "./src"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}
